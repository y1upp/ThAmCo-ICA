@model ThAmCo.Events.Data.Staffing

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Staffing</h4>
    <hr />
    <dl class="row">
        <!-- Display details of the staffing entity -->

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StaffingId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StaffingId)
        </dd>

        <!-- Add similar sections for other properties -->

    </dl>

    <form asp-action="DeleteConfirmed" asp-route-id="@Model.StaffingId" method="post">
        <!-- Include a hidden field for StaffingId -->
        <input type="hidden" asp-for="StaffingId" />

        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-secondary">Cancel</a>
        </div>
    </form>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}